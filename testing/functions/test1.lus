type point = struct {x : int; y : int};

function f(p, q : point) returns (A : point[5]);
function g(in_dist : int) returns (dist : int);

node historically(x : bool) returns (ok : bool);
let
    ok = x and (true -> pre ok);
tel;

node main(p1, p2 : point) returns ();
var
    cex, ok : bool;
    ctr : int;
let
  cex = not (f(p1, p2) = f(p2, p1));
  --%PROPERTY cex;

  ctr = 0 -> (g(pre ctr) + pre ctr);
  ok = historically(10 >= g(ctr) and g(ctr) >= 0) => ctr <= 100;
  --%PROPERTY ok;
tel;
